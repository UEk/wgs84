{
    "name": "@UEk/wgs84",
    "version": "0.5.2",
    "description": "Basic library for switching between cartesian and WGS84 coordiantes",
    "author": "Ulrik Ekedahl <ulrik.ekedahl@gmail.com>",
    "license": "MIT",
    "main": "dist/index.js",
    "directories": {
        "test": "test",
        "src": "src"
    },
    "type": "commonjs",
    "scripts": {
        "audit": "npm audit --registry=https://registry.npmjs.org",
        "lint": "npx eslint .",
        "lint_fix": "npx eslint . --fix",
        "style": "npx prettier --check \"./**/*.ts\" --check \"./**/*.json\" --check \"./**/*.md\"",
        "style_fix": "npx prettier --write \"./**/*.ts\" --write \"./**/*.json\" --write \"./**/*.md\"",
        "verify_and_install": "npm cache verify && npm install",
        "clean": "npx shx rm -rf ./coverage/ ./report/ ./dist/",
        "build": "tsc --build tsconfig.json",
        "test": "npm run clean && npx jest --clearCache && jest --verbose --coverage --color --modulePathIgnorePatterns=./dist/ && npm run build",
        "type_doc": "npx typedoc src/index.ts --excludeNotDocumented"
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "cross-env": "7.0.3",
        "eslint": "^8.12.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "26.1.3",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort-class-members": "^1.14.1",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-unicorn": "^41.0.1",
        "eslint-plugin-yaml": "^0.5.0",
        "jest": "^27.5.1",
        "jest-junit": "^13.0.0",
        "prettier": "^2.6.1",
        "shx": "^0.3.4",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "typedoc": "^0.22.13",
        "typescript": "^4.6.3"
    }
}
